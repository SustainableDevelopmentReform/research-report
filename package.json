{
  "name": "observable-framework-template",
  "version": "1.0.0",
  "description": "A template for creating data-driven reports and dashboards with Observable Framework",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rimraf src/.observablehq/cache",
    "build": "observable build",
    "dev": "observable preview",
    "deploy": "observable deploy",
    "observable": "observable",
    "pdf:export": "cd pdf-export && npm run export",
    "pdf:build": "npm run build && npm run pdf:export",
    "pdf:full": "npm run build && npm run pdf:export && npm run build",
    "pdf:watch": "cd pdf-export && npm run export:watch",
    "pdf:install": "cd pdf-export && npm install"
  },
  "dependencies": {
    "@observablehq/framework": "^1.13.3",
    "d3": "^7.9.0",
    "d3-dsv": "^3.0.1",
    "d3-sankey": "^0.12.3",
    "d3-time-format": "^4.1.0",
    "katex": "^0.16.22",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-katex": "^2.0.3"
  },
  "devDependencies": {
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18"
  }
}
